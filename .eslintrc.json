{
    "extends": ["airbnb", "airbnb/hooks", "prettier"],
    "plugins": ["prettier"],
    "env": {
      "browser": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error",
      "react/react-in-jsx-scope": "off",
      "react/jsx-filename-extension": ["warn", { "extensions": [".jsx", ".js"] }],
      "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
    }
  }
  